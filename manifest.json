{
  "manifest_version": 3,
  "name": "Meaning Search",
  "version": "1.0.1",
  "description": "Right-click selected text to search 'meaning of <text>' on Google.",
  "permissions": ["contextMenus"],
  "background": { "service_worker": "background.js" },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "action": { "default_title": "Meaning Search" }
}
